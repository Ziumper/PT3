<Application x:Class="PT3.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:PT3"
             StartupUri="MainWindow.xaml">
    <Application.Resources>
        <ObjectDataProvider x:Key="Strings"
 ObjectType="{x:Type local:CultureResources}"
 MethodName="GetStringsInstance" />
    </Application.Resources>
</Application>
